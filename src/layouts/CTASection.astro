---
import SectionHeader from "../components/SectionHeader";
// CTA Section Component
---
<section class="bg-MainDark text-white py-20 container mx-auto">
  <div class="container px-4 max-w-7xl">
    <div class="text-center flex flex-col gap-3">
      <!-- Section Header -->
      <SectionHeader 
        subtitle="Let's Get Started"
        title="Ready To Soar To New Heights?"
        titleTag="h2"
        subtitleClass="text-gray-400 h6 tracking-wider uppercase m-0"
        titleClass="h2 font-bold leading-tight m-0"
        align="center"
      />

      <!-- Subheading -->
      <p class="text-xl text-gray-300">
        Start Your Digital Marketing Journey Today With Koi Crest!!
      </p>

      <!-- CTA Button -->
      <div class="pt-6">
        <a
          href="/contact"
          class="inline-block bg-white text-black px-10 py-4 rounded-md font-medium text-lg hover:bg-gray-100 transition duration-200"
        >
          Free Consultation
        </a>
      </div>
    </div>
  </div>
</section>